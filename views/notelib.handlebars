<!doctype html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" 
          content="user-scalable=no,width=device-width" />
          
        
    
    <link href="/css/bootstrap.css" rel="stylesheet">
    <link href="/css/bootstrap-theme.css" rel="stylesheet">
    <link href="/css/notelib.css" rel="stylesheet">
</head>

<body>
    <div class="container">
    <div class="container">
        
        <div class="title_area">
            <h2>Note Library</h2>
        </div>
        
        <div id="map"></div>
        
        <div class="FriendList">
            <hr>
            {{#each notelib}}
            <div class="friendsBox">
                <div class="thisName">
                    <a id="1" href="#"><h4>{{title}}</h4></a>
                </div>
                <div class="thisPage">
                    <text>{{content}}</text>
                </div>
            </div>
            <hr>
            {{/each}}
        </div>
        
        <div class="pageEnd">
            <span style="display:block; height: 14px;"></span>
			<h4><a id="2" href="/" class="button">Back</a></h4>
        </div>
        
        <script src="/js/notelib.js"></script>
        <script src="https://code.jquery.com/jquery.js"></script>
        <script src="/js/bootstrap.js"></script>
        <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAEPQ5go_3a2hSchVOwK3PHFk3I7P1DcMI&callback=initMap"></script>
    </div>
</body>
</html>